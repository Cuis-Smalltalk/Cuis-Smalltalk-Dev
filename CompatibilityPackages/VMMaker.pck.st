			[valToPut := objectMemory integerValueOf: valToPut.
			((valToPut >= 0) and: [valToPut <= 255]) ifFalse: [^ self primitiveFail].
			^ objectMemory storeByte: index - 1 ofObject: rcvr withValue: valToPut]].

	self primitiveFail! !

