	(objectMemory isPointers: rcvr) ifFalse:[^self primitiveFail].
	(objectMemory fetchClassOf: rcvr) = (objectMemory fetchClassOf: arg) ifFalse:[^self primitiveFail].
	length := objectMemory lengthOf: rcvr.
