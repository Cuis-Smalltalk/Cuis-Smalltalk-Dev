	actions at: 27 + 1 put: #offerMenuFromEsc:.		"escape key"
	actions at: 28 + 1 put: #cursorLeft:.				"left arrow key"
	actions at: 29 + 1 put: #cursorRight:.			"right arrow key"
	actions at: 127 + 1 put: #forwardDelete:.		"winDelete key"
