flushAllButDandDEvents
	| newQueue oldQueue  |
	
	newQueue := SharedQueue new.
	eventQueue ifNil: [
		eventQueue := newQueue.
		^self].
	oldQueue := eventQueue.
	[ oldQueue size > 0 ] whileTrue: [ | item type | 
		item := oldQueue next.
		type := item at: 1.
		type = EventSensor eventTypeDragDropFiles ifTrue: [ newQueue nextPut: item]].
	eventQueue := newQueue.
