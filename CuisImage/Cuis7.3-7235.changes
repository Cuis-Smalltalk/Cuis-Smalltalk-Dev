	"For dashed strokes"
	dashedStrokeBits := onOffBitSequence.
	dashBitCount := numberOfBitsInSequence.
	dashBitLength := lengthOfEachBit.
	dashBitOffset := offset.

	self doPath: pathCommandsBlock ensureClosePath: ensureClosePath.! !
