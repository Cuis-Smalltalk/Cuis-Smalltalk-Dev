		(window class registeredMenuAndShortcutsSpecProviders: self textProviderOrModel class name) ifNotNil: [ :providers |
			providers do: [ :eachMessageSend |
				(eachMessageSend valueWithPossibleArgument: self) do: aBlock ]]		.]! !

