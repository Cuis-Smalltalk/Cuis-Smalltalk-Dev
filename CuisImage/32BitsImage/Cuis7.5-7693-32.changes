	innerBarFrame := barFrame insetBy: 2.
	textForm displayAt: textFrame topLeft.
	Display fillBlack: barFrame.
	Display fillWhite: innerBarFrame.
	Display forceToScreen: outerFrame.
	lastDrawTime := Time localMillisecondClock.
