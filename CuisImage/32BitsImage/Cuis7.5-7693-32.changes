	t2 := AffineTransformation from:
		#[ 	0.85 	0.04 	0.00
			-0.04 	0.85 	1.60 ].
	p2 := 0.85.
	
	"Largest left-hand leaflet"
	t3 := AffineTransformation from:
		#[ 	0.20 	-0.26 	0.00
			0.23 	0.22 	1.60 ].
	p3 := 0.07.
	
	"Largest right-hand leaflet "
	t4 := AffineTransformation from:
		#[ 	-0.15 	0.28 	0.00
			0.26 	0.24 	0.44 ].
	p4 := 0.07.! !
